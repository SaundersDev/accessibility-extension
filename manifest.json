{
    "name": "ELEC 490 Chrome Extension",
    "description": "Accessibility Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "menu.html",
        "default_icon": {
            "16": "img/get_started16.png",
            "32": "img/get_started32.png"
        }
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "icons": {
        "16": "img/get_started16.png",
        "32": "img/get_started32.png"
    }
}